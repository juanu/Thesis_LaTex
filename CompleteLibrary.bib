@article{Hasan:2012ca,
author = {Hasan, Nur A and Choi, Seon Young and Eppinger, Mark and Clark, Philip W and Chen, Arlene and Alam, Munirul and Haley, Bradd J and Taviani, Elisa and Hine, Erin and Su, Qi and Tallon, Luke J and Prosper, Joseph B and Furth, Keziah and Hoq, M M and Li, Huai and Fraser-Liggett, Claire M and Cravioto, Alejandro and Huq, Anwar and Ravel, Jacques and Cebula, Thomas A and Colwell, Rita R},
title = {{Genomic diversity of 2010 Haitian cholera outbreak strains.}},
journal = {Proceedings of the National Academy of Sciences},
year = {2012},
month = jun,
affiliation = {Maryland Pathogen Research Institute, University of Maryland, College Park, MD 20742.},
doi = {10.1073/pnas.1207359109},
pmid = {22711841},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-07-13T00:13:01GMT},
date-modified = {2013-06-04T18:02:50GMT},
abstract = {The millions of deaths from cholera during the past 200 y, coupled with the morbidity and mortality of cholera in Haiti since October 2010, are grim reminders that Vibrio cholerae, the etiologic agent of cholera, remains a scourge. We report the isolation of both V. cholerae O1 and non-O1/O139 early in the Haiti cholera epidemic from samples collected from victims in 18 towns across eight Arrondissements of Haiti. The results showed two distinct populations of V. cholerae coexisted in Haiti early in the epidemic. As non-O1/O139 V. cholerae was the sole pathogen isolated from 21% of the clinical specimens, its role in this epidemic, either alone or in concert with V. cholerae O1, cannot be dismissed. A genomic approach was used to examine similarities and differences among the Haitian V. cholerae O1 and V. cholerae non-O1/O139 strains. A total of 47 V. cholerae O1 and 29 V. cholerae non-O1/O139 isolates from patients and the environment were sequenced. Comparative genome analyses of the 76 genomes and eight reference strains of V. cholerae isolated in concurrent epidemics outside Haiti and 27 V. cholerae genomes available in the public database demonstrated substantial diversity of V. cholerae and ongoing flux within its genome.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22711841&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2012/Hasan/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%202012%20Hasan.pdf},
file = {{Proceedings of the National Academy of Sciences 2012 Hasan.pdf:/Users/juan/Documents/Papers2/2012/Hasan/Proceedings of the National Academy of Sciences 2012 Hasan.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1207359109}}
}

@article{Engel:2012cd,
author = {Engel, Philipp and Martinson, Vincent G and Moran, Nancy A},
title = {{Functional diversity within the simple gut microbiota of the honey bee.}},
journal = {Proceedings of the National Academy of Sciences},
year = {2012},
volume = {109},
number = {27},
pages = {11002--11007},
month = jul,
affiliation = {Department of Ecology and Evolutionary Biology, Yale University, New Haven, CT 06520.},
doi = {10.1073/pnas.1202970109},
pmid = {22711827},
language = {English},
rating = {0},
date-added = {2012-07-13T00:15:21GMT},
date-modified = {2013-06-04T18:02:50GMT},
abstract = {Animals living in social communities typically harbor a characteristic gut microbiota important for nutrition and pathogen defense. Accordingly, in the gut of the honey bee, Apis mellifera, a distinctive microbial community, composed of a taxonomically restricted set of species specific to social bees, has been identified. Despite the ecological and economical importance of honey bees and the increasing concern about population declines, the role of their gut symbionts for colony health and nutrition is unknown. Here, we sequenced the metagenome of the gut microbiota of honey bees. Unexpectedly, we found a remarkable degree of genetic diversity within the few bacterial species colonizing the bee gut. Comparative analysis of gene contents suggests that different species harbor distinct functional capabilities linked to host interaction, biofilm formation, and carbohydrate breakdown. Whereas the former two functions could be critical for pathogen defense and immunity, the latter one might assist nutrient utilization. In a $\gamma$-proteobacterial species, we identified genes encoding pectin-degrading enzymes likely involved in the breakdown of pollen walls. Experimental investigation showed that this activity is restricted to a subset of strains of this species providing evidence for niche specialization. Long-standing association of these gut symbionts with their hosts, favored by the eusocial lifestyle of honey bees, might have promoted the genetic and functional diversification of these bee-specific bacteria. Besides revealing insights into mutualistic functions governed by the microbiota of this important pollinator, our findings indicate that the honey bee can serve as a model for understanding more complex gut-associated microbial communities.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22711827&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2012/Engel/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%202012%20Engel.pdf},
file = {{Proceedings of the National Academy of Sciences 2012 Engel.pdf:/Users/juan/Documents/Papers2/2012/Engel/Proceedings of the National Academy of Sciences 2012 Engel.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1202970109}}
}


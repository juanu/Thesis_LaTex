language = {English},
rating = {0},
date-added = {2008-09-23T06:09:55GMT},
date-modified = {2013-06-04T18:00:39GMT},
abstract = {Complex emergent systems of many interacting components, including complex biological systems, have the potential to perform quantifiable functions. Accordingly, we define "functional information," I(E-x), as a measure of system complexity. For a given system and function, x (e.g., a folded RNA sequence that binds to GTP), and degree of function, E-x (e.g., the RNA-GTP binding energy), I(E-x) = -log(2)[F(E-x)], where F(E-x) is the fraction of all possible configurations of the system that possess a degree of function >= E-x. Functional information, which we illustrate with letter sequences, artificial life, and biopolymers, thus represents the probability that an arbitrary configuration of a system will achieve a specific function to a specified degree. In each case we observe evidence for several distinct solutions with different maximum degrees of function, features that lead to steps in plots of information versus degree of function.},
local-url = {file://localhost/Users/juan/Documents/Papers2/2007/Hazen/Proc%20Natl%20Acad%20Sci%20USA%202007%20Hazen.pdf},
file = {{Proc Natl Acad Sci USA 2007 Hazen.pdf:/Users/juan/Documents/Papers2/2007/Hazen/Proc Natl Acad Sci USA 2007 Hazen.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.0701744104}}
}


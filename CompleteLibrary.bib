file = {{FEMS Microbiol Rev 2009 Vieites.pdf:/Users/juan/Documents/Papers2/2009/Vieites/FEMS Microbiol Rev 2009 Vieites.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1574-6976.2008.00152.x}}

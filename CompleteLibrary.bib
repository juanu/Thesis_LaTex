@article{PRONK:1991vg,
author = {PRONK, JT and MEIJER, WM and HAZEU, W and VANDIJKEN, JP and BOS, P and KUENEN, JG},
title = {{GROWTH OF THIOBACILLUS-FERROOXIDANS ON FORMIC-ACID}},
journal = {Applied and Environmental Microbiology},
year = {1991},
volume = {57},

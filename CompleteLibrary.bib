@article{Schafer:1999us,
author = {Sch{\"a}fer, G and Engelhard, M and M{\"u}ller, V},
title = {{Bioenergetics of the Archaea}},
journal = {Microbiology and molecular biology reviews : MMBR},
year = {1999},
volume = {63},
number = {3},
pages = {570--620},
month = sep,
affiliation = {Institut f{\"u}r Biochemie, Medizinische Universit{\"a}t zu L{\"u}beck, L{\"u}beck, Germany. schaefer@biochem.mu-luebeck.de},
pmid = {10477309},
language = {eng},
rating = {0},
date-added = {2008-09-23T05:55:55GMT},
date-modified = {2013-06-04T18:00:12GMT},
abstract = {In the late 1970s, on the basis of rRNA phylogeny, Archaea (archaebacteria) was identified as a distinct domain of life besides Bacteria (eubacteria) and Eucarya. Though forming a separate domain, Archaea display an enormous diversity of lifestyles and metabolic capabilities. Many archaeal species are adapted to extreme environments with respect to salinity, temperatures around the boiling point of water, and/or extremely alkaline or acidic pH. This has posed the challenge of studying the molecular and mechanistic bases on which these organisms can cope with such adverse conditions. This review considers our cumulative knowledge on archaeal mechanisms of primary energy conservation, in relationship to those of bacteria and eucarya. Although the universal principle of chemiosmotic energy conservation also holds for Archaea, distinct features have been discovered with respect to novel ion-transducing, membrane-residing protein complexes and the use of novel cofactors in bioenergetics of methanogenesis. From aerobically respiring Archaea, unusual electron-transporting supercomplexes could be isolated and functionally resolved, and a proposal on the organization of archaeal electron transport chains has been presented. The unique functions of archaeal rhodopsins as sensory systems and as proton or chloride pumps have been elucidated on the basis of recent structural information on the atomic scale. Whereas components of methanogenesis and of phototrophic energy transduction in halobacteria appear to be unique to Archaea, respiratory complexes and the ATP synthase exhibit some chimeric features with respect to their evolutionary origin. Nevertheless, archaeal ATP synthases are to be considered distinct members of this family of secondary energy transducers. A major challenge to future investigations is the development of archaeal genetic transformation systems, in order to gain access to the regulation of bioenergetic systems and to overproducers of archaeal membrane proteins as a prerequisite for their crystallization.},
local-url = {file://localhost/Users/juan/Documents/Papers2/1999/Sch%C3%A4fer/Microbiol%20Mol%20Biol%20Rev%201999%20Sch%C3%A4fer.pdf},
file = {{Microbiol Mol Biol Rev 1999 Schäfer.pdf:/Users/juan/Documents/Papers2/1999/Schäfer/Microbiol Mol Biol Rev 1999 Schäfer.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/6A4FE11A-7798-4644-8840-25D0E676D3E2}}
}

@article{Dorador:2008ex,
author = {Dorador, Cristina and Vila, Irma and Imhoff, Johannes F and Witzel, Karl-Paul},
title = {{Cyanobacterial diversity in Salar de Huasco, a high altitude saline wetland in northern Chile: an example of geographical dispersion?}},
journal = {FEMS Microbiology Ecology},
year = {2008},
volume = {64},
number = {3},
pages = {419--432},
month = jun,
affiliation = {Max Planck Institute for Evolutionary Biology, Pl{\"o}n, Germany. cdorado@ucn.cl},
doi = {10.1111/j.1574-6941.2008.00483.x},
pmid = {18410357},
language = {eng},
rating = {0},
date-added = {2008-09-23T06:11:53GMT},
date-modified = {2013-06-04T18:04:00GMT},
abstract = {The diversity of Cyanobacteria in water and sediment samples from four representative sites of the Salar de Huasco was examined using denaturing gradient gel electrophoresis and analysis of clone libraries of 16S rRNA gene PCR products. Salar de Huasco is a high altitude (3800 m altitude) saline wetland located in the Chilean Altiplano. We analyzed samples from a tributary stream (H0) and three shallow lagoons (H1, H4, H6) that contrasted in their physicochemical conditions and associated biota. Seventy-eight phylotypes were identified in a total of 268 clonal sequences deriving from seven clone libraries of water and sediment samples. Oscillatoriales were frequently found in water samples from sites H0, H1 and H4 and in sediment samples from sites H1 and H4. Pleurocapsales were found only at site H0, while Chroococcales were recovered from sediment samples of sites H0 and H1, and from water samples of site H1. Nostocales were found in sediment samples from sites H1 and H4, and water samples from site H1 and were largely represented by sequences highly similar to Nodularia spumigena. We suggest that cyanobacterial communities from Salar de Huasco are unique - they include sequences related to others previously described from the Antarctic, along with others from diverse, but less extreme environments.},
local-url = {file://localhost/Users/juan/Documents/Papers2/2008/Dorador/FEMS%20Microbiol%20Ecol%202008%20Dorador.pdf},
file = {{FEMS Microbiol Ecol 2008 Dorador.pdf:/Users/juan/Documents/Papers2/2008/Dorador/FEMS Microbiol Ecol 2008 Dorador.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1111/j.1574-6941.2008.00483.x}}
}


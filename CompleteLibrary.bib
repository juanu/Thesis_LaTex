}

@article{Griffin:2009cl,
author = {Griffin, John N and Mendez, Veronica and Johnson, Andrew F and Jenkins, Stuart R and Foggo, Andy},
title = {{Functional diversity predicts overyielding effect of species combination on primary productivity}},
journal = {Oikos},

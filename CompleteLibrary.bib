title = {{Molecular basis of transmembrane signalling by sensory rhodopsin II-transducer complex}},
journal = {Nature},
year = {2002},
volume = {419},
number = {6906},
pages = {484--487},
month = oct,
affiliation = {Research Centre J{\"u}lich, Institute of Structural Biology (IBI-2), 52425 J{\"u}lich, Germany.},
doi = {10.1038/nature01109},
pmid = {12368857},
language = {eng},

@article{Tuffin:2009ep,
author = {Tuffin, Marla and Anderson, Dominique and Heath, Cal and Cowan, Don A},
title = {{Metagenomic gene discovery: how far have we moved into novel sequence space?}},
journal = {Biotechnology Journal},
year = {2009},
volume = {4},
number = {12},
pages = {1671--1683},
month = dec,
affiliation = {Institute for Microbial Biotechnology and Metagenomics, Department of Biotechnology, University of Western Cape, Cape town, South Africa.},
doi = {10.1002/biot.200900235},
pmid = {19946882},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-05-02T23:52:55GMT},
date-modified = {2011-05-16T00:56:29GMT},
abstract = {Metagenomics emerged in the late 1990s as a tool for accessing and studying the collective microbial genetic material in the environment. The advent of the technology generated great excitement, as it has provided new opportunities and technologies for studying the wealth of microbial genetic diversity in the environment. Metagenomics has been widely predicted to access new dimensions of protein sequence space. A decade on, we review how far we have actually moved into new sequence space (and other aspects of protein space) using metagenomic tools. While several novel enzyme activities and protein structures have been identified through metagenomic strategies, the greatest advancement has been made in the isolation of novel protein sequences, some of which have no close relatives, form deeply branched lineages and even represent novel families. This is particularly true for glycosyl hydrolases and lipase/esterases, despite the fact that these activities are frequently screened for in metagenomic studies. However, there is much room for improvement in the methods employed and they will need to be addressed so that access to novel biocatalytic activities can be widened.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19946882&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2009/Tuffin/Biotechnol.%20J.%202009%20Tuffin.pdf},
file = {{Biotechnol. J. 2009 Tuffin.pdf:/Users/juan/Documents/Papers2/2009/Tuffin/Biotechnol. J. 2009 Tuffin.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/biot.200900235}}
}


@article{PRONK:1991uf,
author = {PRONK, JT and LIEM, K and BOS, P and KUENEN, JG},
title = {{ENERGY TRANSDUCTION BY ANAEROBIC FERRIC IRON RESPIRATION IN THIOBACILLUS-FERROOXIDANS}},

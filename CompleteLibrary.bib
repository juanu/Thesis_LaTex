@article{Klappenbach:2000vz,
author = {Klappenbach, J A and Dunbar, J M and Schmidt, T M},
title = {{rRNA operon copy number reflects ecological strategies of bacteria.}},
journal = {Applied and Environmental Microbiology},
year = {2000},
volume = {66},
number = {4},
pages = {1328--1333},
month = apr,
affiliation = {Department of Microbiology and Center for Microbial Ecology, Michigan State University, East Lansing, Michigan 48824-1101, USA.},
pmid = {10742207},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-04-30T17:13:31GMT},
date-modified = {2013-06-04T18:02:38GMT},
abstract = {Although natural selection appears to favor the elimination of gene redundancy in prokaryotes, multiple copies of each rRNA-encoding gene are common on bacterial chromosomes. Despite this conspicuous deviation from single-copy genes, no phenotype has been consistently associated with rRNA gene copy number. We found that the number of rRNA genes correlates with the rate at which phylogenetically diverse bacteria respond to resource availability. Soil bacteria that formed colonies rapidly upon exposure to a nutritionally complex medium contained an average of 5.5 copies of the small subunit rRNA gene, whereas bacteria that responded slowly contained an average of 1.4 copies. In soil microcosms pulsed with the herbicide 2,4-dichlorophenoxyacetic acid (2,4-D), indigenous populations of 2,4-D-degrading bacteria with multiple rRNA genes ( = 5.4) became dominant, whereas populations with fewer rRNA genes ( = 2.7) were favored in unamended controls. These findings demonstrate phenotypic effects associated with rRNA gene copy number that are indicative of ecological strategies influencing the structure of natural microbial communities.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10742207&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2000/Klappenbach/Applied%20and%20Environmental%20Microbiology%202000%20Klappenbach.pdf},
file = {{Applied and Environmental Microbiology 2000 Klappenbach.pdf:/Users/juan/Documents/Papers2/2000/Klappenbach/Applied and Environmental Microbiology 2000 Klappenbach.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/F4B47E4F-A644-45A5-8609-029AB552592D}}
}


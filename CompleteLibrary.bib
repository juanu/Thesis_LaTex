journal = {Genome Biology and Evolution},
year = {2010},
volume = {2},
pages = {646--655},
month = jan,
affiliation = {Department of Population Medicine and Diagnostic Sciences, Cornell University, Ithaca, New York, USA.},
doi = {10.1093/gbe/evq048},

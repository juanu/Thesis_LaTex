file = {{eLife 2013 Di Rienzi.pdf:/Users/juan/Documents/Papers2/2013/Di Rienzi/eLife 2013 Di Rienzi.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.7554/eLife.01102}}
}

@article{Nemergut:2013gk,
author = {Nemergut, Diana R and Schmidt, Steven K and Fukami, Tadashi and O'Neill, Sean P and Bilinski, Teresa M and Stanish, Lee F and Knelman, Joseph E and Darcy, John L and Lynch, Ryan C and Wickey, Phillip and Ferrenberg, Scott},
title = {{Patterns and processes of microbial community assembly.}},

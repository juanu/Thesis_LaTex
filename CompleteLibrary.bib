pages = {1711--1723},
month = jul,
affiliation = {Divisi{\'o}n de Microbiolog{\'\i}a, Departamento de Fisiolog{\'\i}a, Gen{\'e}tica y Microbiolog{\'\i}a, Universidad de Alicante, 03080 Alicante, Spain.},
doi = {10.1111/j.1462-2920.2007.01289.x},

@article{CaetanoAnolles:2003kj,
author = {Caetano-Anoll{\'e}s, Gustavo and Caetano-Anoll{\'e}s, Derek},
title = {{An evolutionarily structured universe of protein architecture.}},
journal = {Genome Research},
year = {2003},
volume = {13},
number = {7},
pages = {1563--1571},
month = jul,
affiliation = {Vital NRG, Knoxville, Tennessee 37919, USA. gca@uiuc.edu},
doi = {10.1101/gr.1161903},
pmid = {12840035},
language = {English},
rating = {0},
date-added = {2011-11-01T18:33:00GMT},
date-modified = {2013-06-04T18:02:23GMT},
abstract = {Protein structural diversity encompasses a finite set of architectural designs. Embedded in these topologies are evolutionary histories that we here uncover using cladistic principles and measurements of protein-fold usage and sharing. The reconstructed phylogenies are inherently rooted and depict histories of protein and proteome diversification. Proteome phylogenies showed two monophyletic sister-groups delimiting Bacteria and Archaea, and a topology rooted in Eucarya. This suggests three dramatic evolutionary events and a common ancestor with a eukaryotic-like, gene-rich, and relatively modern organization. Conversely, a general phylogeny of protein architectures showed that structural classes of globular proteins appeared early in evolution and in defined order, the alpha/beta class being the first. Although most ancestral folds shared a common architecture of barrels or interleaved beta-sheets and alpha-helices, many were clearly derived, such as polyhedral folds in the all-alpha class and beta-sandwiches, beta-propellers, and beta-prisms in all-beta proteins. We also describe transformation pathways of architectures that are prevalently used in nature. For example, beta-barrels with increased curl and stagger were favored evolutionary outcomes in the all-beta class. Interestingly, we found cases where structural change followed the alpha-to-beta tendency uncovered in the tree of architectures. Lastly, we traced the total number of enzymatic functions associated with folds in the trees and show that there is a general link between structure and enzymatic function.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=12840035&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2003/Caetano-Anoll%C3%A9s/Genome%20Res%202003%20Caetano-Anoll%C3%A9s.pdf},
file = {{Genome Res 2003 Caetano-Anollés.pdf:/Users/juan/Documents/Papers2/2003/Caetano-Anollés/Genome Res 2003 Caetano-Anollés.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1101/gr.1161903}}
}


@article{Tabita:2008jc,
author = {Tabita, F Robert and Hanson, Thomas E and Satagopan, Sriram and Witte, Brian H and Kreel, Nathan E},
title = {{Phylogenetic and evolutionary relationships of RubisCO and the RubisCO-like proteins and the functional lessons provided by diverse molecular forms}},

@article{Reams:2004bj,
author = {Reams, Andrew B and Neidle, Ellen L},
title = {{Selection for gene clustering by tandem duplication}},
journal = {Annual Review of Microbiology},
year = {2004},
volume = {58},
pages = {119--142},
month = jan,
affiliation = {Section of Microbiology, University of California, Davis, California 95616, USA. abreams@ucdavis.edu},

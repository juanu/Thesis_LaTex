@article{Chattopadhyay:2012in,
author = {Chattopadhyay, Sujay and Paul, Sandip and Kisiela, Dagmara I and Linardopoulou, Elena V and Sokurenko, Evgeni V},
title = {{Convergent molecular evolution of genomic cores in Salmonella enterica and Escherichia coli.}},
journal = {Journal Of Bacteriology},
year = {2012},
volume = {194},
number = {18},
pages = {5002--5011},
month = sep,
affiliation = {Department of Microbiology, University of Washington, Seattle, Washington, USA. sujayc@u.washington.edu},
doi = {10.1128/JB.00552-12},
pmid = {22797756},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-04-07T19:49:33GMT},
date-modified = {2013-06-04T18:02:08GMT},
abstract = {One of the strongest signals of adaptive molecular evolution of proteins is the occurrence of convergent hot spot mutations: repeated changes in the same amino acid positions. We performed a comparative genome-wide analysis of mutation-driven evolution of core (omnipresent) genes in 17 strains of Salmonella enterica subspecies I and 22 strains of Escherichia coli. More than 20% of core genes in both Salmonella and E. coli accumulated hot spot mutations, with a predominance of identical changes having recent evolutionary origin. There is a significant overlap in the functional categories of the adaptively evolving genes in both species, although mostly via separate molecular mechanisms. As a strong evidence of the link between adaptive mutations and virulence in Salmonella, two human-restricted serovars, Typhi and Paratyphi A, shared the highest number of genes with serovar-specific hot spot mutations. Many of the core genes affected by Typhi/Paratyphi A-specific mutations have known virulence functions. For each species, a list of nonrecombinant core genes (and the hot spot mutations therein) under positive selection is provided.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=22797756&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2012/Chattopadhyay/J%20Bacteriol%202012%20Chattopadhyay.pdf},
file = {{J Bacteriol 2012 Chattopadhyay.pdf:/Users/juan/Documents/Papers2/2012/Chattopadhyay/J Bacteriol 2012 Chattopadhyay.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/JB.00552-12}}
}

@article{Chattopadhyay:2009ca,
author = {Chattopadhyay, Sujay and Weissman, Scott J and Minin, Vladimir N and Russo, Thomas A and Dykhuizen, Daniel E and Sokurenko, Evgeni V},
title = {{High frequency of hotspot mutations in core genes of Escherichia coli due to short-term positive selection.}},
journal = {Proceedings of the National Academy of Sciences},
year = {2009},
volume = {106},
number = {30},
pages = {12412--12417},
month = jul,
affiliation = {Department of Microbiology, University of Washington, Seattle, WA 98195, USA.},
doi = {10.1073/pnas.0906217106},
pmid = {19617543},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-04-07T19:50:03GMT},
date-modified = {2014-02-04T02:13:02GMT},
abstract = {Core genes comprising the ubiquitous backbone of bacterial genomes are not subject to frequent horizontal transfer and generally are not thought to contribute to the adaptive evolution of bacterial pathogens. We determined, however, that at least one-third and possibly more than one-half of the core genes in Escherichia coli genomes are targeted by repeated replacement substitutions in the same amino acid positions-hotspot mutations. Occurrence of hotspot mutations is driven by positive selection, as their rate is significantly higher than expected by random chance alone, and neither intragenic recombination nor increased mutability can explain the observed patterns. Also, commensal E. coli strains have a significantly lower frequency of mutated genes and mutations per genome than pathogenic strains. E. coli strains causing extra-intestinal infections accumulate hotspot mutations at the highest rate, whereas the highest total number of mutated genes has been found among Shigella isolates, suggesting the pathoadaptive nature of such mutations. The vast majority of hotspot mutations are of recent evolutionary origin, implying short-term positive selection, where adaptive mutations emerge repeatedly but are not sustained in natural circulation for long. Such pattern of dynamics is consistent with source-sink model of virulence evolution.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19617543&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2009/Chattopadhyay/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%202009%20Chattopadhyay.pdf},
file = {{Proceedings of the National Academy of Sciences 2009 Chattopadhyay.pdf:/Users/juan/Documents/Papers2/2009/Chattopadhyay/Proceedings of the National Academy of Sciences 2009 Chattopadhyay.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.0906217106}}
}


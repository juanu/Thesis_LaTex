file = {{Chem Rec 2004 Sakuraba.pdf:/Users/juan/Documents/Papers2/2004/Sakuraba/Chem Rec 2004 Sakuraba.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/tcr.10066}}
}


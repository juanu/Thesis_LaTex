@article{AtamnaIsmaeel:2008cr,
author = {Atamna-Ismaeel, Nof and Sabehi, Gazalah and Sharon, Itai and Witzel, Karl-Paul and Labrenz, Matthias and J{\"u}rgens, Klaus and Barkay, Tamar and Stomp, Maayke and Huisman, Jef and Beja, Oded},

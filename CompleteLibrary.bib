file = {{Lancet 2010 Ashley.pdf:/Users/juan/Documents/Papers2/2010/Ashley/Lancet 2010 Ashley.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/S0140-6736(10)60452-7}}
}


@article{Heras:2011ih,
author = {Heras, Joseph and Koop, Ben F and Aguilar, Andres},
title = {{A transcriptomic scan for positively selected genes in two closely related marine fishes: Sebastes caurinus and S. rastrelliger.}},
journal = {Marine genomics},
year = {2011},
volume = {4},
number = {2},
pages = {93--98},
month = jun,
affiliation = {School of Natural Sciences and Graduate Group in Quantitative and Systems Biology, University of California Merced, 5200 N. Lake Rd., Merced, CA 95344, USA.},
doi = {10.1016/j.margen.2011.02.001},
pmid = {21620330},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-11-15T20:47:35GMT},
date-modified = {2014-02-27T05:35:49GMT},
abstract = {Comparative genomic analyses can provide valuable insight into functional evolutionary divergence among closely related species. Here we employ a comparative evolutionary analysis of expressed sequence tags (ESTs) from two closely related species of marine fishes (genus Sebastes--rockfish). Sebastes is a highly diverse group of marine fishes that inhabit a wide array of marine habitats and the study of this group can provide insights into speciation in the marine environment. ESTs were developed for S. caurinus (23,668 from brain, kidney, and spleen tissues) and S. rastrelliger (11,207 from brain and pituitary tissues). Following assembly we were able to identify, with high confidence, 257 orthologous sequence pairs between the two species through a reciprocal best hit blast search. An analysis of functional divergence between orthologs revealed that 19.46% had Ka/Ks values greater than 0.5 and 8.17% had Ka/Ks values greater than one, identifying a large pool of candidate genes to further study adaptive divergence in the group. Genes with elevated Ka/Ks values belonged to the following functional categories: immune function, metabolism, longevity, and reproductive behavior, indicating that adaptive divergence in these functional groups may be important in the diversification of this group of fishes. This study provides the ground work to better understand the molecular evolution of genes involved in a radiation of marine fishes.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21620330&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2011/Heras/Mar%20Genomics%202011%20Heras.pdf},
file = {{Mar Genomics 2011 Heras.pdf:/Users/juan/Documents/Papers2/2011/Heras/Mar Genomics 2011 Heras.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.margen.2011.02.001}}
}


file = {{Microbiol Mol Biol Rev 2013 Nemergut.pdf:/Users/juan/Documents/Papers2/2013/Nemergut/Microbiol Mol Biol Rev 2013 Nemergut.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1128/MMBR.00051-12}}

@article{Hansen:2011dn,
author = {Hansen, Elizabeth E and Lozupone, Catherine A and Rey, Federico E and Wu, Meng and Guruge, Janaki L and Narra, Aneesha and Goodfellow, Jonathan and Zaneveld, Jesse R and McDonald, Daniel T and Goodrich, Julia A and Heath, Andrew C and Knight, Rob and Gordon, Jeffrey I},
title = {{Pan-genome of the dominant human gut-associated archaeon, Methanobrevibacter smithii, studied in twins.}},
journal = {Proceedings of the National Academy of Sciences},
year = {2011},
volume = {108 Suppl 1},
pages = {4599--4606},
month = mar,
affiliation = {Center for Genome Sciences and Systems Biology, Washington University School of Medicine, St. Louis, MO 63108, USA.},
doi = {10.1073/pnas.1000071108},
pmid = {21317366},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-06-12T19:15:48GMT},
date-modified = {2013-06-04T18:02:44GMT},
abstract = {The human gut microbiota harbors three main groups of H(2)-consuming microbes: methanogens including the dominant archaeon, Methanobrevibacter smithii, a polyphyletic group of acetogens, and sulfate-reducing bacteria. Defining their roles in the gut is important for understanding how hydrogen metabolism affects the efficiency of fermentation of dietary components. We quantified methanogens in fecal samples from 40 healthy adult female monozygotic (MZ) and 28 dizygotic (DZ) twin pairs, analyzed bacterial 16S rRNA datasets generated from their fecal samples to identify taxa that co-occur with methanogens, sequenced the genomes of 20 M. smithii strains isolated from families of MZ and DZ twins, and performed RNA-Seq of a subset of strains to identify their responses to varied formate concentrations. The concordance rate for methanogen carriage was significantly higher for MZ versus DZ twin pairs. Co-occurrence analysis revealed 22 bacterial species-level taxa positively correlated with methanogens: all but two were members of the Clostridiales, with several being, or related to, known hydrogen-producing and -consuming bacteria. The M. smithii pan-genome contains 987 genes conserved in all strains, and 1,860 variably represented genes. Strains from MZ and DZ twin pairs had a similar degree of shared genes and SNPs, and were significantly more similar than strains isolated from mothers or members of other families. The 101 adhesin-like proteins (ALPs) in the pan-genome (45 $\pm$ 6 per strain) exhibit strain-specific differences in expression and responsiveness to formate. We hypothesize that M. smithii strains use their different repertoires of ALPs to create diversity in their metabolic niches, by allowing them to establish syntrophic relationships with bacterial partners with differing metabolic capabilities and patterns of co-occurrence.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21317366&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/2011/Hansen/Proceedings%20of%20the%20National%20Academy%20of%20Sciences%202011%20Hansen.pdf},
file = {{Proceedings of the National Academy of Sciences 2011 Hansen.pdf:/Users/juan/Documents/Papers2/2011/Hansen/Proceedings of the National Academy of Sciences 2011 Hansen.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1000071108}}
}


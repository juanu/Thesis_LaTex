@article{Sickmeier:2007fh,
author = {Sickmeier, Megan and Hamilton, Justin A and LeGall, Tanguy and Vacic, Vladimir and Cortese, Marc S and Tantos, Agnes and Szabo, Beata and Tompa, Peter and Chen, Jake and Uversky, Vladimir N and Obradovi{\'c}, Zoran and Dunker, A Keith},
title = {{DisProt: the Database of Disordered Proteins.}},
journal = {Nucleic Acids Research},

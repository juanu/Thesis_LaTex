file = {{DNA Res 2005 Abe.pdf:/Users/juan/Documents/Papers2/2005/Abe/DNA Res 2005 Abe.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1093/dnares/dsi015}}
}

@article{Chan:2008kh,

@article{Delong:1986wj,
author = {Delong, E F and Yayanos, A A},
title = {{Biochemical function and ecological significance of novel bacterial lipids in deep-sea procaryotes.}},
journal = {Applied and Environmental Microbiology},
year = {1986},
volume = {51},
number = {4},
pages = {730--737},
month = apr,
affiliation = {Scripps Institution of Oceanography, University of California at San Diego, La Jolla, California 92093.},
pmid = {16347037},
language = {English},
read = {Yes},
rating = {0},
date-added = {2013-02-20T00:47:39GMT},
date-modified = {2013-06-04T18:02:00GMT},
abstract = {The fatty acid composition of the membrane lipids in 11 deep-sea bacterial isolates was determined. The fatty acids observed were typical of marine vibrios except for the presence of large amounts of long-chain polyunsaturated fatty acids (PUFAs). These long-chain PUFAs were previously thought to be absent in procaryotes, with the notable exception of a single marine Flexibacter sp. In three barophilic strains tested at 2 degrees C, there was a general increase in the relative amount of PUFAs as pressure was increased from a low growth pressure towards the optimal growth pressure. In Vibrio marinus MP-1, a psychrophilic strain, PUFAs were found to increase as a function of decreasing temperature at constant atmospheric pressure. These results suggest the involvement of PUFAs in the maintenance of optimal membrane fluidity and function over environmentally relevant temperatures and pressures. Furthermore, since these lipids are essential nutrients for higher taxa and are found in large amounts in the lipids of deep-sea vertebrates and invertebrates, an important, specific role for deep-sea bacteria in abyssal food webs is implicated.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16347037&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/juan/Documents/Papers2/1986/Delong/Applied%20and%20Environmental%20Microbiology%201986%20Delong.pdf},
file = {{Applied and Environmental Microbiology 1986 Delong.pdf:/Users/juan/Documents/Papers2/1986/Delong/Applied and Environmental Microbiology 1986 Delong.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/9B265002-EFCF-42C4-AB6A-9AC9A3B6A602}}
}


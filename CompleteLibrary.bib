author = {Temperton, B and Field, D and Oliver, A and Tiwari, B and M{\"u}hling, M and Joint, I and Gilbert, J},
title = {{Bias in assessments of marine microbial biodiversity in fosmid libraries as evaluated by pyrosequencing}},
journal = {The ISME Journal},
year = {2009},
month = apr,
affiliation = {1Plymouth Marine Laboratory, Plymouth, UK.},
doi = {10.1038/ismej.2009.32},

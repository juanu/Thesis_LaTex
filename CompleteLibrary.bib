@article{Abel:2005dv,
author = {Abel, David L and Trevors, Jack T},
title = {{Three subsets of sequence complexity and their relevance to biopolymeric information}},
journal = {Theoretical biology {\&} medical modelling},
year = {2005},
volume = {2},
pages = {29},
month = jan,
affiliation = {The Gene Emergence Project, The Origin-of-Life Foundation, Inc., Greenbelt, MD 20770-1610, USA. life@us.net},
doi = {10.1186/1742-4682-2-29},
pmid = {16095527},
language = {eng},
rating = {0},
date-added = {2008-09-23T06:00:18GMT},
date-modified = {2013-06-04T18:00:18GMT},
abstract = {Genetic algorithms instruct sophisticated biological organization. Three qualitative kinds of sequence complexity exist: random (RSC), ordered (OSC), and functional (FSC). FSC alone provides algorithmic instruction. Random and Ordered Sequence Complexities lie at opposite ends of the same bi-directional sequence complexity vector. Randomness in sequence space is defined by a lack of Kolmogorov algorithmic compressibility. A sequence is compressible because it contains redundant order and patterns. Law-like cause-and-effect determinism produces highly compressible order. Such forced ordering precludes both information retention and freedom of selection so critical to algorithmic programming and control. Functional Sequence Complexity requires this added programming dimension of uncoerced selection at successive decision nodes in the string. Shannon information theory measures the relative degrees of RSC and OSC. Shannon information theory cannot measure FSC. FSC is invariably associated with all forms of complex biofunction, including biochemical pathways, cycles, positive and negative feedback regulation, and homeostatic metabolism. The algorithmic programming of FSC, not merely its aperiodicity, accounts for biological organization. No empirical evidence exists of either RSC of OSC ever having produced a single instance of sophisticated biological organization. Organization invariably manifests FSC rather than successive random events (RSC) or low-informational self-ordering phenomena (OSC).},
local-url = {file://localhost/Users/juan/Documents/Papers2/2005/Abel/Theoretical%20biology%20&%20medical%20modelling%202005%20Abel.pdf},
file = {{Theoretical biology & medical modelling 2005 Abel.pdf:/Users/juan/Documents/Papers2/2005/Abel/Theoretical biology & medical modelling 2005 Abel.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1186/1742-4682-2-29}}
}


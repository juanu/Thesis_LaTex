@article{Logares:2012jg,
author = {Logares, Ramiro and Haverkamp, Thomas H A and Kumar, Surendra and Lanz{\'e}n, Anders and Nederbragt, Alexander J and Quince, Christopher and Kauserud, H{\aa}vard},

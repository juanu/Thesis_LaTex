uri = {\url{papers2://publication/doi/10.1016/j.bpj.2008.11.026}}
}

@article{Hoff:1995vp,
author = {Hoff, W D and Matthijs, H C and Schubert, H and Crielaard, W and Hellingwerf, K J},
title = {{Rhodopsin(s) in eubacteria}},

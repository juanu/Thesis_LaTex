@article{Rohwerder:2003tq,
author = {Rohwerder, Thore and Sand, Wolfgang},
title = {{The sulfane sulfur of persulfides is the actual substrate of the sulfur-oxidizing enzymes from Acidithiobacillus and Acidiphilium spp}},
journal = {Microbiology (Reading, England)},
year = {2003},
volume = {149},
number = {Pt 7},
pages = {1699--1710},
month = jul,
affiliation = {Department of Microbiology, Institute for General Botany, University of Hamburg, Ohnhorststr 18, D-22609 Hamburg, Germany. rohwerd@umb.ufz.de},
pmid = {12855721},
language = {eng},
rating = {0},
date-added = {2008-09-23T05:58:00GMT},
date-modified = {2013-06-04T18:00:16GMT},
abstract = {To identify the actual substrate of the glutathione-dependent sulfur dioxygenase (EC 1.13.11.18) elemental sulfur oxidation of the meso-acidophilic Acidithiobacillus thiooxidans strains DSM 504 and K6, Acidithiobacillus ferrooxidans strain R1 and Acidiphilium acidophilum DSM 700 was analysed. Extraordinarily high specific sulfur dioxygenase activities up to 460 nmol x min(-1) (mg protein)(-1) were found in crude extracts. All cell-free systems oxidized elemental sulfur only via glutathione persulfide (GSSH), a non-enzymic reaction product from glutathione (GSH) and elemental sulfur. Thus, GSH plays a catalytic role in elemental sulfur activation, but is not consumed during enzymic sulfane sulfur oxidation. Sulfite is the first product of sulfur dioxygenase activity; it further reacted non-enzymically to sulfate, thiosulfate or glutathione S-sulfonate (GSSO(-3)). Free sulfide was not oxidized by the sulfur dioxygenase. Persulfide as sulfur donor could not be replaced by other sulfane-sulfur-containing compounds (thiosulfate, polythionates, bisorganyl-polysulfanes or monoarylthiosulfonates). The oxidation of H(2)S by the dioxygenase required GSSG, i.e. the disulfide of GSH, which reacted non-enzymically with sulfide to give GSSH prior to enzymic oxidation. On the basis of these results and previous findings a biochemical model for elemental sulfur and sulfide oxidation in Acidithiobacillus and Acidiphilium spp. is proposed.},
local-url = {file://localhost/Users/juan/Documents/Papers2/2003/Rohwerder/Microbiology%20(Reading%20Engl)%202003%20Rohwerder.pdf},
file = {{Microbiology (Reading Engl) 2003 Rohwerder.pdf:/Users/juan/Documents/Papers2/2003/Rohwerder/Microbiology (Reading Engl) 2003 Rohwerder.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/DCA0E6D1-A7CC-4654-9F2E-A2FBB0F7E7A2}}
}

@article{JessicaLGreen:2008ja,
author = {Jessica L Green and Brendan J M Bohannan and Whitaker, Rachel J},
title = {{Microbial biogeography: from taxonomy to traits}},
journal = {Science},
year = {2008},
volume = {320},
number = {5879},
pages = {1039--1043},
month = may,
affiliation = {Center for Ecology and Evolutionary Biology, Department of Biology, University of Oregon, Eugene, Oregon 97403, USA. jlgreen@uoregon.edu},
doi = {10.1126/science.1153475},
pmid = {18497288},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2008-09-23T06:12:04GMT},
date-modified = {2011-05-16T00:56:32GMT},
abstract = {The biogeographic variation of life has predominantly been studied using taxonomy, but this focus is changing. There is a resurging interest in understanding patterns in the distribution not only of taxa but also of the traits those taxa possess. Patterns of trait variation shed light on fundamental questions in biology, including why organisms live where they do and how they will respond to environmental change. Technological advances such as environmental genomics place microbial ecology in a unique position to move trait-based biogeography forward. We anticipate that as trait-based biogeography continues to evolve, micro- and macroorganisms will be studied in concert, establishing a science that is informed by and relevant to all domains of life.},
local-url = {file://localhost/Users/juan/Documents/Papers2/2008/Jessica%20L%20Green/Science%202008%20Jessica%20L%20Green.pdf},

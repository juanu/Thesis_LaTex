title = {{Prokaryotic diversity in Tuz Lake, a hypersaline environment in Inland Turkey}},
journal = {FEMS Microbiology Ecology},
year = {2008},
volume = {65},
number = {3},
pages = {474--483},
month = sep,
affiliation = {Divisi{\'o}n de Microbiolog{\'\i}a, Universidad de Alicante, Alicante, Spain.},

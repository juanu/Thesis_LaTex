month = jul,
affiliation = {Abteilung Bioinformatik, Institut f{\"u}r Mikrobiologie und Genetik, Georg-August-Universit{\"a}t G{\"o}ttingen, Goldschmidtstr. 1, 37077 G{\"o}ttingen, Germany. katharina@gobics.de},

year = {2009},
volume = {106},
number = {21},
pages = {8605--8610},
month = may,
affiliation = {Department of Microbiology and Institute for Genomic Biology, University of Illinois at Urbana-Champaign, Urbana, IL 61801, USA.},
doi = {10.1073/pnas.0808945106},
